@{
    ViewBag.Title = "Create";
}
<section class="content">
    <h2 class="mt-0 m-b">Reservation</h2>
    <div class="box box-info">
        <div class="box-header with-border">
            <h3 class="box-title">Reservation Details</h3>
        </div>
        <!-- /.box-header -->
        <!-- form start -->
        <form class="">
            <div class="box-body">
                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Title</label>
                        <select class="form-control">
                            <option>--Select Title--</option>
                            <option>Ms.</option>
                            <option>Mr.</option>
                            <option>Mrs.</option>
                            <option>Miss</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Last Name *</label>
                        <input type="text" class="form-control" id="" placeholder="Last Name">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">First Name</label>
                        <input type="text" class="form-control" id="" placeholder="First Name">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Member Type</label>
                        <select class="form-control">
                            <option>--Select --</option>
                        </select>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Country</label>
                        <select class="form-control">
                            <option>--Select Country--</option>
                            <option>US</option>
                            <option>India</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Language</label>
                        <select class="form-control">
                            <option>--Select Language--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">VIP</label>
                        <select class="form-control">
                            <option>--Select VIP--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Phone</label>
                        <input type="text" class="form-control" id="" placeholder="Phone">
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Member No</label>
                        <input type="text" class="form-control" id="" placeholder="Member No">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Member LVT.</label>
                        <input type="text" class="form-control" id="" placeholder="Member LVT.">
                    </div>

                    <div class="col-md-3 form-group">
                        <label for="" class="">Agent</label>
                        <select class="form-control">
                            <option>--Select Agent--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Company</label>
                        <select class="form-control">
                            <option>--Select Company--</option>
                        </select>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Group</label>
                        <select class="form-control">
                            <option>--Select Group--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Source</label>
                        <select class="form-control">
                            <option>--Select Source--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Contact</label>
                        <select class="form-control">
                            <option>--Select Contact--</option>
                        </select>
                    </div>
                </div>

                <div class="clearfix"><hr /></div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Arrival Date *</label>
                        <div class="input-group date">
                            <input type="text" class="form-control" id="datetimepicker4" placeholder="MM/dd/yyyy">
                            <div class="input-group-addon">
                                <i class="fa fa-calendar"></i>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-1 form-group">
                        <label for="" class="">Nights</label>
                        <input type="text" class="form-control" id="" placeholder="1">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Departure Date *</label>
                        <div class="input-group date">
                            <input type="text" class="form-control" id="datetimepicker4" placeholder="MM/dd/yyyy">
                            <div class="input-group-addon">
                                <i class="fa fa-calendar"></i>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-1 form-group">
                        <label for="" class="">Adults</label>
                        <input type="text" class="form-control" id="" placeholder="1">
                    </div>
                    <div class="col-md-1 form-group">
                        <label for="" class="">Child</label>
                        <input type="text" class="form-control" id="" placeholder="0">
                    </div>

                    <div class="col-md-3 form-group">
                        <label for="" class="">Room Type *</label>
                        <select class="form-control">
                            <option>--Select Room Type--</option>
                        </select>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">No Of Rooms.</label>
                        <input type="text" class="form-control" id="" placeholder="1">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">RTC.</label>
                        <select class="form-control">
                            <option>--Select RTC--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Room</label>
                        <select class="form-control">
                            <option>--Select Room--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Extn.</label>
                        <select class="form-control">
                            <option>--Select Extn--</option>
                        </select>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Rate Code *</label>
                        <input type="text" class="form-control" id="" placeholder="CORP">
                    </div>
                    
                    <div class="col-md-3 form-group">
                        <label for="" class="">Rate</label>
                        <input type="text" class="form-control" id="" placeholder="90.00">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Curr.</label>
                        <select class="form-control">
                            <option>--Select Currency--</option>
                            <option>USD</option>
                        </select>
                    </div>

                    <div class="col-md-3 form-group m-t-lg m-r-n-lg">
                        <label class="container1">
                            Fixed Rate
                            <input type="checkbox">
                            <span class="checkmark"></span>
                        </label>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Packages</label>
                        <input type="text" class="form-control" id="" placeholder="">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Block Code</label>
                        <input type="text" class="form-control" id="" placeholder="">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">ETA</label>
                        <input type="text" class="form-control" id="" placeholder="">
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Res. Type *</label>
                        <select class="form-control">
                            <option>--Select Res. Type--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Market *</label>
                        <select class="form-control">
                            <option>--Select Market--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Source *</label>
                        <select class="form-control">
                            <option>--Select Source--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Origin</label>
                        <select class="form-control">
                            <option>--Select Origin--</option>
                        </select>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Payment *</label>
                        <select class="form-control">
                            <option>--Select Payment--</option>
                        </select>
                    </div>

                    <div class="col-md-3 form-group">
                        <label for="" class="">Credit Card Number</label>
                        <input type="text" class="form-control" id="" placeholder="11111">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Exp. Date</label>
                        <div class="input-group date">
                            <input type="text" class="form-control" id="datetimepicker4">
                            <div class="input-group-addon">
                                <i class="fa fa-calendar"></i>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">CRS No.</label>
                        <input type="text" class="form-control" id="" placeholder="">
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Approval Code</label>
                        <input type="text" class="form-control" id="" placeholder="">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Approval Amt.</label>
                        <input type="text" class="form-control" id="" placeholder="">
                    </div>

                    <div class="col-md-3 form-group">
                        <label for="" class="">Suite With</label>
                        <input type="text" class="form-control" id="" placeholder="">
                    </div>
                    <div class="col-md-3 form-group m-t-lg m-r-n-lg">
                        <label class="container1">
                            Email Confirmation
                            <input type="checkbox">
                            <span class="checkmark"></span>
                        </label>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">Guest Balance</label>
                        <input type="text" class="form-control" id="" placeholder="">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="col-xs-12 padding-zero">Disc. Amount</label>

                        <div class="col-md-5 col-sm-5 padding-zero">
                            <input type="text" class="form-control" id="" placeholder="">
                        </div>
                        <div class="col-md-2 col-sm-2 padding-zero text-center">
                            <span>%</span>
                        </div>
                        <div class="col-md-5 col-sm-5 padding-zero">
                            <input type="text" class="form-control" id="" placeholder="">
                        </div>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Reason</label>
                        <select class="form-control">
                            <option>--Select Reason--</option>
                        </select>
                    </div>

                </div>

                <div class="row">
                    <div class="col-md-3 form-group">
                        <label for="" class="">TA Rec Loc</label>
                        <input type="text" class="form-control" id="" placeholder="0">
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Specials</label>
                        <select class="form-control">
                            <option>--Select Specials--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Comments</label>
                        <select class="form-control">
                            <option>--Select Comments--</option>
                        </select>
                    </div>
                    <div class="col-md-3 form-group">
                        <label for="" class="">Item Inv.</label>
                        <input type="text" class="form-control" id="" placeholder="">
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-9 form-group">
                        <label for="txtRemarks">Remarks</label>
                        <textarea id="txtRemarks" class="form-control" rows="3" placeholder="Remarks"></textarea>
                    </div>
                    <div class="col-xs-3 form-group m-t-xl m-t-md_media">
                        <button type="button" class="btn btn-info btn-sm">
                            Preference
                        </button>
                    </div>
                </div>
                <div class="row" style="display:none;" id="divSelectedPreference">
                    <div class="col-md-12 form-group">
                        <label for="ListOfSelectedPreferences" class="m-l-n col-sm-2 control-label m-r-n-xl"> Selected Preference</label>
                        <div class="col-sm-10">
                            <p id="ListOfSelectedPreferences"></p>
                        </div>
                    </div>
                </div>


            </div>
            <!-- /.box-body -->
            <div class="box-footer text-center">
                <button type="submit" class="btn btn-success">Save</button>
                <button type="submit" class="btn btn-danger">Cancel</button>
            </div>
            <!-- /.box-footer -->
        </form>
    </div>
</section>

<!-- This is the add reservation form-->