@model SuccessHotelierHub.Models.ReservationVM
@using SuccessHotelierHub.Models
@using SuccessHotelierHub.Utility
@{
    ViewBag.Title = "Edit Reservation";

    var preferenceGroupList = (List<SelectListItem>)ViewBag.PreferenceGroupList;

    var arrivalDayOfWeek = string.Empty;
    var departureDayOfWeek = string.Empty;

    if (Model.ArrivalDate != null && Model.ArrivalDate.HasValue)
    {
        arrivalDayOfWeek = SuccessHotelierHub.Utility.Utility.WeekdayName((int)Model.ArrivalDate.Value.DayOfWeek);
    }

    if (Model.DepartureDate != null && Model.DepartureDate.HasValue)
    {
        departureDayOfWeek = SuccessHotelierHub.Utility.Utility.WeekdayName((int)Model.DepartureDate.Value.DayOfWeek);
    }

    string ETAText = string.Empty;

    if (Model.ETA.HasValue && Model.ETA.Value != null)
    {
        DateTime time = DateTime.Today.Add(Model.ETA.Value);
        ETAText = time.ToString("HH:mm");
    }

    var strRate = "0.00";
    if (Model.Rate.HasValue)
    {
        strRate = string.Format("{0:0.00}", Model.Rate.Value);
    }

    #region Preference Mapping
    var selectedPreferences = (List<ReservationPreferenceMappingVM>)ViewBag.SelectedPreferences;
    var preferenceItems = string.Empty;
    var selectedPreferencesHtml = string.Empty;

    if (selectedPreferences != null && selectedPreferences.Count > 0)
    {
        foreach (var preference in selectedPreferences)
        {
            preferenceItems += string.Format("{0},", preference.PreferenceId);

            selectedPreferencesHtml += string.Format("<span class='btn btn-xs bg-maroon m-r-xs' id='spnPreference_{1}'>{0}<i class='fa fa-remove m-l-sm' onclick='removePreferenceItem(this)' data-preferenceid='{1}'></i></span>", preference.PreferenceDescription, preference.PreferenceId);
        }

        if (!string.IsNullOrWhiteSpace(preferenceItems))
        {
            //Remove Last Comma.
            preferenceItems = Utility.RemoveLastCharcter(preferenceItems, ',');
        }
    }
    #endregion

    #region Room Mapping

    var selectedRooms = (List<ReservationRoomMappingVM>)ViewBag.SelectedRooms;
    var roomIds = string.Empty;
    var roomNumbers = string.Empty;

    if (selectedRooms != null && selectedRooms.Count > 0)
    {
        foreach (var room in selectedRooms)
        {
            roomIds += string.Format("{0},", room.RoomId);
            roomNumbers += string.Format("{0}, ", room.RoomNo);
        }

        if (!string.IsNullOrWhiteSpace(roomIds))
        {
            //Remove Last Comma.
            roomIds = Utility.RemoveLastCharcter(roomIds, ',');
        }

        if (!string.IsNullOrWhiteSpace(roomNumbers))
        {
            //Remove Last Comma.
            roomNumbers = Utility.RemoveLastCharcter(roomNumbers, ',');
        }
    }

    #endregion

    #region Add Ons Mapping
    var selectedAddOns = (List<ReservationAddOnsMappingVM>)ViewBag.SelectedAddOns;
    var selectedAddOnsHtml = string.Empty;

    if (selectedAddOns != null && selectedAddOns.Count > 0)
    {
        foreach (var addOns in selectedAddOns)
        {
            selectedAddOnsHtml += string.Format("<span class='btn btn-xs bg-purple m-r-xs' id='spnAddOns_{1}'>{0}<i class='fa fa-remove m-l-sm' onclick='removeAddOnsItem(this)' data-addonsid='{1}' data-price='{2}' data-addons='{3}'></i></span>", addOns.AddOnsName, addOns.AddOnsId, CurrencyManager.ParseAmountToUserCurrency(addOns.AddOnsPrice, LogInManager.CurrencyCode), addOns.AddOnsName);
        }
    }
    #endregion

    #region Package Mapping

    var packageMapping = new ReservationPackageMappingVM();
    string packageBeginDate = "", packageEndDate = "";

    if (ViewBag.PackageMapping != null)
    {
        packageMapping = (ReservationPackageMappingVM)ViewBag.PackageMapping;
        packageBeginDate = packageMapping.BeginDate.HasValue ? packageMapping.BeginDate.Value.ToString("dd/MM/yyyy") : "";
        packageEndDate = packageMapping.EndDate.HasValue ? packageMapping.EndDate.Value.ToString("dd/MM/yyyy") : "";

        packageMapping.PackagePrice = CurrencyManager.ParseAmountToUserCurrency(packageMapping.PackagePrice, LogInManager.CurrencyCode);
        packageMapping.TotalAmount = CurrencyManager.ParseAmountToUserCurrency(packageMapping.TotalAmount, LogInManager.CurrencyCode);

        Model.PackageName = packageMapping.PackageName;
    }

    #endregion

    #region Manage Source Parameter
    var sourceParam = Convert.ToString(Request.QueryString["source"]);
    var qid = string.Empty;
    var btnCancelUrl = Url.Action("List", "Reservation");

    if (!string.IsNullOrWhiteSpace(sourceParam))
    {
        if (sourceParam == "RoomPlan")
        {
            btnCancelUrl = Url.Action("RoomPlan", "Reservation");
        }
        if (sourceParam == "SearchArrivals")
        {
            btnCancelUrl = Url.Action("Arrivals", "FrontDesk");
        }
        if (sourceParam == "RegistrationCard")
        {
            btnCancelUrl = Url.Action("RegistrationCard", "Report");
        }
        if (sourceParam == "BreakfastReport")
        {
            btnCancelUrl = Url.Action("Breakfast", "Report");
        }
    }
    #endregion

    #region Reservation Status

    var reservationStatus = "";
    if (Model.IsReservationCancel)
    {
        reservationStatus = "Cancelled";
    }
    else if (Model.IsCheckIn && !Model.IsCheckOut)
    {
        reservationStatus = "Checked In";
    }
    else if (Model.IsCheckIn && Model.IsCheckOut)
    {
        reservationStatus = "Checked Out";
    }

    #endregion

    var packageList = (List<PackageVM>)ViewBag.PackageList;
    var childrenCharge = CurrencyManager.ParseAmountToUserCurrency((double)Constants.CHILDREN_CHARGE, LogInManager.CurrencyCode);


}


